<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.viral.vetogame" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".GameList"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewGame"
            android:label="New Game"
            android:parentActivityName=".GameList" >
        </activity>
        <activity
            android:name=".InvitePeople"
            android:label="Invite"
            android:parentActivityName=".NewGame" >
        </activity>
        <activity
            android:name=".InitSuggestion"
            android:label="Suggestion"
            android:parentActivityName=".NewGame" >
        </activity>
    </application>

</manifest>
